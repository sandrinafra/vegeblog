<h1>profile</h1>

<p><%= @user.firstname %></p>
<p><%= @user.lastname %></p>
<p><%= @user.email %></p>
<% if current_user.user? %>
	<%= link_to "edit", edit_user_registration_path %>
<% end %>

<h1>My comments</h1>

<% @user.comments.each do |comment| %>
	<p><%= comment.text %></p>
<% end %>





<h1>Comments I'd like</h1>

<% @user.likes.each do |like| %>
	<p><%= like.comment.text %></p>
<% end %>